{
  "expo": {
    "name": "UcmsApp",
    "slug": "UcmsApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "moc.ppasmcu",
      "buildNumber": "1.0.0",
      "$comment": "下記ではAppStoreで日本語表記にならないのでInfo.plistに直接記述",
      "infoPlist": {
        "CFBundleDevelopmentRegion":"ja_JP"
      }
    },
    "android": {
      "package": "moc.ppasmcu",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-adaptive-foreground.png",
        "backgroundImage": "./assets/icon-adaptive-background.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "sentry-expo",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sound": "./assets/sound.wav"
        }
      ]
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "ucmsapp",
            "project": "ucmsapp",
            "authToken": "b313152b9d5a410792b2490eed55dad6bafa3935671e4b62b3b5f77d3ebd17c9"
          }
        }
      ]
    },
    "userInterfaceStyle": "automatic",
    "extra": {
      "eas": {
        "projectId": "b34077c0-96bb-4a63-b78e-78c9b4c979b6"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "ios_app_id": "ca-app-pub-6389398835875193~2739631520",
    "delay_app_measurement_init": true,
    "user_tracking_usage_description": "許可することで、より関連性の高い広告を表示することができるようになります"
  }
}
